<style>
        form {
            margin: 20px;
            padding: 20px;
            border: 1px solid #ccc;
            border-radius: 5px;
            background-color: #0f2f99;
            width: 300px;
        }
    
        label {
            display: block;
            margin-bottom: 5px;
            font-weight: bold;
        }
    
        input[type="text"],
        input[type="number"],
        input[type="date"],
        input[type="submit"] {
            margin-bottom: 10px;
